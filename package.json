{
  "name": "ronanlevesque",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronanlevesque/ronanlevesque.git"
  },
  "description": "My personal website, built with Gatsby.js",
  "version": "0.1.0",
  "author": "Ronan Levesque <ronan.levesque@gmail.com>",
  "bugs": {
    "url": "https://github.com/ronanlevesque/ronanlevesque/issues"
  },
  "scripts": {
    "build": "yarn clean:css && gatsby build",
    "build:css": "node_modules/.bin/fragments build src/css && node fluid-css.config.js",
    "clean:css": "yarn build:css && node_modules/.bin/purgecss --config purgecss.config.js --out src/css",
    "lint": "eslint src",
    "serve": "yarn clean:css && gatsby serve",
    "start": "yarn build:css && gatsby develop"
  },
  "dependencies": {
    "@algolia/fragments.js": "^2.0.0-beta.9",
    "@emotion/core": "^10.0.10",
    "classnames": "^2.2.6",
    "gatsby": "^2.20.18",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-feed": "^2.4.3",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-preact": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-json": "^2.3.1",
    "gatsby-transformer-remark": "^2.7.1",
    "preact": "^10.3.3",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-algolia": "^13.2.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "prettier": "^1.16.4",
    "purgecss": "^1.3.0",
    "webpack": "^4.42.1"
  },
  "homepage": "https://ronanlevesque.com",
  "license": "MIT"
}
